package com.zyx1011.mobilesafe002.entity;

import android.graphics.drawable.Drawable;

/**
 * 应用信息实体类
 * 
 * @author zhongyuxin
 */
public class SoftwareInfo {
	private Drawable icon; // 图标
	private String name; // 应用名
	private String packageName; // 包名
	private long apkSize; // 应用大小
	private boolean isInstallSD; // 安装位置
	private boolean isSystem; // 应用类型

	public SoftwareInfo() {
	}

	public SoftwareInfo(Drawable ico, String name, String packageName, long apkSize, boolean isInstallSD,
			boolean isSystem) {
		this.icon = ico;
		this.name = name;
		this.packageName = packageName;
		this.apkSize = apkSize;
		this.isInstallSD = isInstallSD;
		this.isSystem = isSystem;
	}

	public Drawable getIco() {
		return icon;
	}

	public void setIco(Drawable ico) {
		this.icon = ico;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPackageName() {
		return packageName;
	}

	public void setPackageName(String packageName) {
		this.packageName = packageName;
	}

	public boolean isInstallSD() {
		return isInstallSD;
	}

	public void setInstallSD(boolean isInstallSD) {
		this.isInstallSD = isInstallSD;
	}

	public boolean isSystem() {
		return isSystem;
	}

	public void setSystem(boolean isSystem) {
		this.isSystem = isSystem;
	}

	public long getApkSize() {
		return apkSize;
	}

	public void setApkSize(long apkSize) {
		this.apkSize = apkSize;
	}

	@Override
	public String toString() {
		return "SoftwareInfo [ico=" + icon + ", name=" + name + ", packageName=" + packageName + ", apkSize=" + apkSize
				+ ", isInstallSD=" + isInstallSD + ", isSystem=" + isSystem + "]";
	}

}
